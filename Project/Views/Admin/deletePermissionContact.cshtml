@model FeedbackTb
<!-- container section start -->
<section id="container" class="container">

    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            <h2 class="mt-4 text-center text-uppercase text-danger" style="letter-spacing:4px;">Are you sure you want to delete this Feedback?</h2>

            <!-- page start-->
            <div class="row">
                <div class="col-sm-12">
                    <section class="panel">
                        <header class="panel-heading">
                            Delete User
                        </header>
                        <div class="col-md-12">
                            <div class="panel-body">
                                <ul class="list-group mt-1">
                                    <li class="list-group-item">S.No : @Model.feedback_id</li>
                                    <li class="list-group-item">User Name : @Model.user_name </li>
                                    <li class="list-group-item">User Email : @Model.user_email </li>
                                    <li class="list-group-item">User Country : @Model.user_country </li>
                                    <li class="list-group-item">User Message : @Model.user_message </li>


                                </ul>
                                <a href="@Url.Action("deleteContact","Admin",new{id=Model.feedback_id})" class="btn btn-danger">Delete Feedback</a>
                                <a href="@Url.Action("fetchContact","Admin")" class="btn btn-primary">Back to list</a>
                            </div> 
                        </div>
                    </section>
                </div>
            </div>

        </section>
    </section>
</section>
