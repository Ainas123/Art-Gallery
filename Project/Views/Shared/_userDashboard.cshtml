<!DOCTYPE html>
<html>
<head>
    <title>Art Gallery Management System - Admin Dashboard</title>

    <!-- Bootstrap CSS -->
    <link href="~/style/user/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="~/style/user/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="~/style/user/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="~/style/user/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
    <!-- full calendar css-->
    <link href="~/style/user/assets/fullcalendar/fullcalendar/bootstrap-fullcalendar.css" rel="stylesheet" />
    <link href="~/style/user/assets/fullcalendar/fullcalendar/fullcalendar.css" rel="stylesheet" />
    <!-- easy pie chart-->
    <link href="~/style/user/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <!-- owl carousel -->
    <link rel="stylesheet" href="~/style/user/css/owl.carousel.css" type="text/css">
    <link href="~/style/user/css/jquery-jvectormap-1.2.2.css" rel="stylesheet">
    <!-- Custom styles -->
    <link rel="stylesheet" href="~/style/user/css/fullcalendar.css">
    <link href="~/style/user/css/widgets.css" rel="stylesheet">
    <link href="~/style/user/css/style.css" rel="stylesheet">
    <link href="~/style/user/css/style-responsive.css" rel="stylesheet" />
